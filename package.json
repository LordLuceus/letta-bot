{
  "name": "letta-bot",
  "version": "1.0.0",
  "description": "A Discord bot frontend for a Letta agent",
  "main": "dist/index.js",
  "scripts": {
    "start": "tsc && node .",
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "lint": "eslint ./src --ext .ts",
    "lint:fix": "eslint ./src --ext .ts --fix",
    "format": "prettier --write ./src",
    "format:check": "prettier --check ./src",
    "prepare": "husky || true"
  },
  "keywords": [
    "discord",
    "bot",
    "letta",
    "typescript",
    "AI",
    "agent",
    "elevenlabs",
    "voice"
  ],
  "author": {
    "email": "lord.luceus@gmail.com",
    "name": "LordLuceus",
    "url": "https://github.com/lordluceus/letta-bot"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lordluceus/letta-bot.git"
  },
  "license": "MIT",
  "packageManager": "pnpm@10.12.4",
  "dependencies": {
    "@elevenlabs/elevenlabs-js": "^2.8.0",
    "@letta-ai/letta-client": "^0.1.176",
    "discord.js": "^14.21.0",
    "dotenv": "^17.2.1",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@types/node": "^24.2.0",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "prettier": "^3.6.2",
    "tsx": "^4.20.3",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.39.0"
  }
}
